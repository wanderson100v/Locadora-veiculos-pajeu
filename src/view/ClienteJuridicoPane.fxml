<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<FlowPane stylesheets="@folhaEstilo.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ClienteJuridicoController">
   <children>
      <TableView fx:id="entidadeTabela" onMouseClicked="#mouseClikHandle" prefHeight="330.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="nomeCln" prefWidth="75.0" text="Nome" />
          <TableColumn fx:id="cnpjCln" prefWidth="75.0" text="CNPJ" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox fx:id="entidadePane" prefWidth="505.0">
         <children>
            <GridPane alignment="CENTER" hgap="10.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="388.0" minWidth="10.0" prefWidth="325.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="388.0" minWidth="10.0" prefWidth="325.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Label text="Email" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></Label>
                  <Label text="Telefone" GridPane.rowIndex="5">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></Label>
                  <FlowPane hgap="5.0" GridPane.rowIndex="6">
                     <children>
                        <TextField fx:id="telPreFld" prefHeight="25.0" prefWidth="49.0">
                           <font>
                              <Font name="Verdana" size="12.0" />
                           </font></TextField>
                        <TextField fx:id="telNumFld" prefHeight="25.0" prefWidth="183.0">
                           <font>
                              <Font name="Verdana" size="12.0" />
                           </font></TextField>
                     </children>
                  </FlowPane>
                  <TextField fx:id="emailFld" GridPane.columnIndex="1" GridPane.rowIndex="4">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="Inscrição Estadual" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></Label>
                  <TextField fx:id="inscicaoEstadualFld" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="CNPJ" GridPane.rowIndex="3">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></Label>
                  <TextField fx:id="cnpjFld" prefHeight="25.0" prefWidth="269.0" GridPane.rowIndex="4">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="Nome" GridPane.rowIndex="1">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></Label>
                  <TextField fx:id="nomeFld" GridPane.rowIndex="2">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="Pessoa Jurídica" GridPane.columnSpan="2" GridPane.halignment="CENTER">
                     <font>
                        <Font name="Verdana" size="14.0" />
                     </font></Label>
                  <GridPane GridPane.columnIndex="1" GridPane.rowIndex="5" GridPane.rowSpan="2">
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints maxHeight="42.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints maxHeight="31.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                     <children>
                        <Label text="Ativo" />
                        <FlowPane alignment="CENTER_LEFT" hgap="10.0" prefHeight="36.0" prefWidth="97.0" GridPane.rowIndex="1">
                           <children>
                              <RadioButton fx:id="simAtivoRb" mnemonicParsing="false" selected="true" text="Sim" />
                              <RadioButton fx:id="naoAtivoRb" mnemonicParsing="false" text="Não" />
                           </children>
                        </FlowPane>
                        <Label text="Código" GridPane.columnIndex="1" />
                        <TextField fx:id="codigoFld" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                     </children>
                  </GridPane>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </padding>
            </GridPane>
            <Separator prefWidth="200.0" />
            <GridPane alignment="CENTER" hgap="10.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="358.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="363.0" minWidth="10.0" prefWidth="358.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" />
               </padding>
               <children>
                  <Label text="Rua" GridPane.rowIndex="3" />
                  <TextField fx:id="ruaFld" GridPane.rowIndex="4">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="Bairro" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Label text="Cidade" GridPane.rowIndex="5" />
                  <TextField fx:id="bairroFld" GridPane.columnIndex="1" GridPane.rowIndex="4">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <TextField fx:id="cidadeFld" GridPane.rowIndex="6">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="Estado" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                  <ComboBox fx:id="estadoBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <Label text="Número" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></Label>
                  <TextField fx:id="numFld" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="CEP" GridPane.rowIndex="1">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></Label>
                  <TextField fx:id="cepFld" GridPane.rowIndex="2">
                     <font>
                        <Font name="Verdana" size="12.0" />
                     </font></TextField>
                  <Label text="Endereço" GridPane.columnSpan="2" GridPane.halignment="CENTER">
                     <font>
                        <Font name="Verdana" size="14.0" />
                     </font></Label>
               </children>
            </GridPane>
         </children>
      </VBox>
      <ButtonBar fx:id="acoesBar" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button fx:id="limparBtn" mnemonicParsing="false" onAction="#crudHandle" text="Limpar">
               <font>
                  <Font name="Verdana" size="12.0" />
               </font>
            </Button>
            <Button fx:id="cadastrarBtn" mnemonicParsing="false" onAction="#crudHandle" text="Cadastrar">
               <font>
                  <Font name="Verdana" size="12.0" />
               </font>
            </Button>
            <Button fx:id="editarBtn" mnemonicParsing="false" onAction="#crudHandle" text="Editar">
               <font>
                  <Font name="Verdana" size="12.0" />
               </font>
            </Button>
            <Button fx:id="excluirBtn" mnemonicParsing="false" onAction="#crudHandle" text="Excluir">
               <font>
                  <Font name="Verdana" size="12.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
   </children>
</FlowPane>
